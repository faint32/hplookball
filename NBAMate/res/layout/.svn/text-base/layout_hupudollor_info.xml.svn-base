<?xml version="1.0" encoding="utf-8"?>
<!-- 球员的打分列表页 -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ff2d2d2d"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/layout_title_bar"
        style="@style/bg_title_bar" >

        <Button
            android:id="@+id/btn_back"
            style="@style/btn_back" />

        <TextView
            android:id="@+id/txt_title"
            style="@style/txt_title"
            android:text="@string/title_hupudollor_log" />
        <Button
            android:id="@+id/btn_contacts"
            style="@style/btn_right"
            android:text="@string/title_contacts"
            android:visibility="gone"
            android:textColor="@color/res_cor1" />
    </RelativeLayout>

    <com.hupu.games.view.HupuWebView
        android:id="@+id/coin_info_webview"
        android:layout_below="@+id/layout_title_bar"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" />
    
    <ProgressBar
        android:id="@+id/probar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
       />
</RelativeLayout>